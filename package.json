{
  "name": "agile-assessment-tool",
  "version": "1.0.0",
  "description": "Agile Assessment Tool Zuhlke Singapore",
  "main": "index.js",
  "scripts": {
    "clean-build": "rimraf build",
    "start": "node ./src/server.js",
    "start:dev": "nodemon ./src/server.js & npm run start:client:dev",
    "start:client:dev": "cd ./src/client && npm start",
    "build:dev": "npm i && cd ./src/client && npm i",
    "build:prod": "npm run build-client && npm install",
    "build-client": "cd ./src/client && npm install && npm run build && npm run clean-build && mv build ../../",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
