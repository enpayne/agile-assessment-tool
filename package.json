{
  "name": "agile-assessment-tool",
  "version": "1.0.0",
  "description": "Agile Assessment Tool Zuhlke Singapore",
  "main": "index.js",
  "scripts": {
    "start": "node ./src/server.js",
    "start:e2e": "NODE_ENV=test node ./src/server.js",
    "start:dev": "npm run start:server:dev & npm run start:client:dev",
    "start:server:dev": "nodemon ./src/server.js",
    "start:client:dev": "cd ./src/client && npm start",
    "build:dev": "npm i && cd ./src/client && npm i",
    "build:prod": "npm run build-client:prod && npm install",
    "build-client:prod": "cd ./src/client && npm install && npm run build && rimraf ../../build && mv build ../../build",
    "test": "jest --forceExit",
    "test:watch": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://enpayne@github.com/enpayne/agile-assessment-tool.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/enpayne/agile-assessment-tool/issues"
  },
  "homepage": "https://github.com/enpayne/agile-assessment-tool#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "4.16.4",
    "mongodb": "3.1.13",
    "mongodb-memory-server": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-jest": "^24.1.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "rimraf": "^2.6.3",
    "supertest": "^3.4.2"
  }
}
